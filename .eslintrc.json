{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [],
    "settings": {},
    "ignorePatterns": [
        "tests/*.test.js"
    ],
    "rules": {
        "max-len": [
            "error",
            {
                "code": 100
            }
        ]
    },
    "overrides": [
        {
            "files": [
                "*.js"
            ]
        }
    ]
}